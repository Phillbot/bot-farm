{
  "name": "bot-hatchery",
  "version": "0.8.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "start:dev": "npx nodemon",
    "start:dev:all": "npx nodemon --config nodemon-all.json",
    "start:all": "node dist/index-all.js",
    "start:nbu-bot": "node dist/index-nbu-bot.js",
    "start:react-clicker-bot": "node dist/index-react-clicker.js",
    "build": "rm -rf ./dist && tsc && tsc-alias && node helpers/copy-dir.ts",
    "migrate:nbu": "sequelize-cli --config config/config.js db:migrate --env nbu",
    "migrate:nbu:undo": "sequelize-cli --config config/config.js db:migrate:undo --env nbu",
    "migrate:react-clicker": "sequelize-cli --config config/config.js db:migrate --env reactClicker",
    "migrate:react-clicker:undo": "sequelize-cli --config config/config.js db:migrate:undo --env reactClicker",
    "prettier": "prettier --write \"**/*.+(ts|js|jsx|tsx|css|scss|json|yml|html)\"",
    "prettier:check": "prettier -c \"**/*.+(ts|js|jsx|tsx|css|scss|json|yml|html)\"",
    "lint": "eslint  --ext .tsx,.ts --fix src/"
  },
  "keywords": [],
  "author": "Phillbot",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.17",
    "@types/node": "20.5.9",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "nodemon": "3.0.1",
    "pino-pretty": "13.1.2",
    "prettier": "3.2.4",
    "rimraf": "5.0.1",
    "sequelize-cli": "^6.6.3",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.8",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@grammyjs/emoji": "1.2.0",
    "@grammyjs/i18n": " 1.1.2",
    "axios": "1.6.7",
    "chart.js": "3.5.1",
    "chartjs-node-canvas": "4.1.6",
    "chartjs-plugin-datalabels": "2.2.0",
    "cors": "^2.8.5",
    "cron": "3.1.6",
    "dotenv": "16.3.1",
    "ejs": "3.1.10",
    "express": "4.18.2",
    "express-rate-limit": "7.5.0",
    "grammy": "1.34.0",
    "inversify": "6.0.2",
    "pg": "8.11.3",
    "pino": "8.21.0",
    "prettytable.js": "1.0.1",
    "reflect-metadata": "0.2.1",
    "sequelize": "6.35.2",
    "some-random-cat": "2.4.0",
    "uniqolor": "1.1.1"
  }
}
